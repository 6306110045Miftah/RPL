package modul8;

import java.util.Scanner;

/**
 *
 * @author Asisten
 */
public class Modul8 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input = new
                Scanner(System.in);
                System.out.println("Masukkan angka :");
                int n=input.nextInt();
                int a =1, b =0;
                System.out.print("deretnya adalah :");
                for(int i=0;i<n;i++){
                    System.out.print(a+"");
                    a=a+b;
                    b=a-b;
                }
                System.out.println();
        
        }
    }
    
